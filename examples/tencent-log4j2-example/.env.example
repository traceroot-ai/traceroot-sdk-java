# Required: Tencent Cloud credentials
TENCENT_SECRET_ID=your_tencent_secret_id
TENCENT_SECRET_KEY=your_tencent_secret_key

# Required: Tencent APM trace token (for Tencent trace provider)
TRACE_TOKEN=your_trace_token

# Optional: Tencent Cloud CLS logset name (like AWS log group)
TENCENT_LOGSET=your_logset_name

# Absolute path to your project root directory
TRACEROOT_ROOT_PATH=your_project_path_here
